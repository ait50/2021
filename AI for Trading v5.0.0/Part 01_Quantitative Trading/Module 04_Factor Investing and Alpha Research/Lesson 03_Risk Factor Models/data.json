        {
          "id": 752340,
          "key": "52808062-de33-4f20-aab8-44a135e9f73a",
          "title": "Portfolio Variance using Factor Model",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "52808062-de33-4f20-aab8-44a135e9f73a",
            "completed_at": null,
            "last_viewed_at": null,
