        {
          "id": 752307,
          "key": "7df149f7-86fd-48f1-90b6-210094b0dbdb",
          "title": "Risk Factors v. Alpha Factors",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "7df149f7-86fd-48f1-90b6-210094b0dbdb",
            "completed_at": null,
