            {
              "id": 752497,
              "key": "0fb0456e-9368-4560-92bd-7ede8f0536e1",
              "title": "M4 L1A 08 Rescale Part 2 V3",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "8Ix10U6MEug",
