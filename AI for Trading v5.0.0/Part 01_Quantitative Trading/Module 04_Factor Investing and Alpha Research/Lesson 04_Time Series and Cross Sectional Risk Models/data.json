            {
              "id": 771065,
              "key": "a57390fb-38fa-401b-93fd-0f036bdc9201",
              "title": "M4 L2A 17 Fama French Size Factor V3",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "FXZuHsn0bx4",
                "china_cdn_id": "FXZuHsn0bx4.mp4"
