            {
              "id": 692051,
              "key": "f8574b2a-b3c5-42d3-af01-b8bb6f8d8bb0",
              "title": "Testing For Normalilty",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "Sa1MJegyYfc",
