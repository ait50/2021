            {
              "id": 840135,
              "key": "751547d5-f23a-4b78-9601-d0ea0fe2e524",
              "title": "M8l2 07 Tcost Part1 V1",
              "semantic_type": "VideoAtom",
              "is_public": true,
              "instructor_notes": "",
              "video": {
                "youtube_id": "E8di2Qu9Ogo",
