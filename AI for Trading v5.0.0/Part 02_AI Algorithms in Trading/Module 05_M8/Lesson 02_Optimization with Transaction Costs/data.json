              "height": 693,
              "instructor_notes": null
            },
            {
              "id": 830621,
              "key": "006d23f7-11f5-4bc2-bd35-8e0100946315",
              "title": null,
              "semantic_type": "ImageAtom",
              "is_public": true,
              "url": "https://video.udacity-data.com/topher/2019/February/5c6cc3b9_tcost-2/tcost-2.png",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/006d23f7-11f5-4bc2-bd35-8e0100946315",
              "caption": "",
              "alt": "",
