        {
          "id": 825919,
          "key": "edc26477-7fa9-4f02-b9dc-0dd6e35bdbd7",
          "title": "Review Multi-Factor Models",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "edc26477-7fa9-4f02-b9dc-0dd6e35bdbd7",
