              "url": "https://video.udacity-data.com/topher/2018/January/5a62711e_screen-shot-2018-01-19-at-2.28.03-pm/screen-shot-2018-01-19-at-2.28.03-pm.png",
              "non_google_url": "https://s3.cn-north-1.amazonaws.com.cn/u-img/90b4555b-c6dd-45fd-b9af-6dab9c063b00",
              "caption": "",
